{
  "name": "express-rest-api",
  "version": "1.0.0",
  "description": "Express REST API is a simple REST API with Express framework and TypeScript for Node.js, use mysql with prisma to store data.",
  "main": "src/app.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node ./dist/app.js",
    "start:dev": "npx nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "prisma migrate dev",
    "migrate:test": "dotenv -e .env.test prisma migrate dev",
    "test": "dotenv -e .env.test -- jest -i"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@eslint/js": "^9.12.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/ms": "^0.7.34",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0"
  }
}
